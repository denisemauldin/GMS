<h1>Listing file_types</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Type name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @file_types.each do |file_type| %>
  <tr>
    <td><%= file_type.id %></td>
    <td><%= file_type.type_name %></td>
    <td><%= link_to 'Show', file_type %></td>
    <td><%= link_to 'Edit', edit_file_type_path(file_type) %></td>
    <td><%= link_to 'Destroy', file_type, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

