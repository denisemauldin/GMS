<h1>Listing memberships</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Pedigree</th>
    <th>Person</th>
    <th>Draw duplicate</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @memberships.each do |membership| %>
  <tr>
    <td><%= membership.id %></td>
    <td><%= membership.pedigree_id %></td>
    <td><%= membership.person_id %></td>
    <td><%= membership.draw_duplicate %></td>
    <td><%= link_to 'Show', membership %></td>
    <td><%= link_to 'Edit', edit_membership_path(membership) %></td>
    <td><%= link_to 'Destroy', membership, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Membership', new_membership_path %>
